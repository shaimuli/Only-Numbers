<?xml version='1.0' encoding='utf-8'?>
<widget xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps" id="io.cordova.MobileFLS" version="1.0.0" xmlns="http://www.w3.org/ns/widgets">
  <name>Only-Numbers</name>
  <author href="http://cordova.io" email="dev@cordova.apache.org">Only-Numbers</author>
  <content src="index.html" />
  <icon src="icon.png" />

  <icon src="res/icon/android/icon-36-ldpi.png" />
  <icon src="res/icon/android/icon-48-mdpi.png" />
  <icon src="res/icon/android/icon-72-hdpi.png"/>
  <icon src="res/icon/android/icon-96-xhdpi.png" />
  <access origin="*" />
 <vs:toolsetVersion>6.3.1</vs:toolsetVersion>
  <engine name="android" spec="~5.2.1" />
  <engine name="ios" spec="4.2.0" />
  <engine name="windows" spec="4.4.2" />
  <preference name="SplashScreen" value="screen" />
  <preference name="windows-target-version" value="10.0" />
  <!-- Support for Cordova 5.0.0 plugin system -->
  <plugin name="cordova-plugin-whitelist" spec="1.2.2" />
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <allow-intent href="tel:*" />
  <allow-intent href="sms:*" />
  <allow-intent href="mailto:*" />
  <allow-intent href="geo:*" />
  <platform name="android">
    <allow-intent href="market:*" />
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
  </platform>
</widget>
