<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.JetCopy" version="1.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
    <feature name="StatusBar">
        <param name="android-package" onload="true" value="org.apache.cordova.statusbar.StatusBar" />
    </feature>
    <name>JetCopy</name>
    <description>JetCopy</description>
    <author email="aaa@aaa.apache.org" href="http://cordova.io">JetCopy</author>
    <content src="index.html" />
    <access origin="*" />
    <icon src="icon.png" />
    <icon src="res/icon/android/icon-36-ldpi.png" />
    <icon src="res/icon/android/icon-48-mdpi.png" />
    <icon src="res/icon/android/icon-72-hdpi.png" />
    <icon src="res/icon/android/icon-96-xhdpi.png" />
    <preference name="SplashScreen" value="screen" />
    <preference name="windows-target-version" value="8.0" />
    <preference name="windows-phone-target-version" value="8.1" />
    <preference name="android-minSdkVersion" value="7" />
    <vs:plugin name="org.apache.cordova.inappbrowser" version="0.5.2" />
    <vs:plugin name="org.apache.cordova.dialogs" version="0.2.10" />
    <vs:plugin name="org.apache.cordova.statusbar" version="0.1.8" />
    <vs:platformSpecificValues />
</widget>
